{
  "version": 3,
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/avatars/(.*)",
      "dest": "/avatars/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
